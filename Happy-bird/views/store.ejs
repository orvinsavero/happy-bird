        .error {
            margin: 10px;
            text-shadow: 0 2px 8px 0 rgb(58, 55, 55);
            color: rgba(247, 0, 0, 0.76);
            margin-top: 15px;

            text-align: center;
            font-family: 'Open Sans', sans-serif;
            font-weight: 700;
        }

        .success {
            margin: 10px;
            text-shadow: 0 2px 8px 0 rgb(58, 55, 55);
            color: rgba(91, 243, 31, 0.753);
            margin-top: 15px;

            text-align: center;
            font-family: 'Open Sans', sans-serif;
            font-weight: 700;
        }

        body {
            background-image: url('/images/sky.png');
        }

        table {
            border-collapse: collapse;

            width: 400px;
            margin-top: 20px;
            border-collapse: collapse;
            border-radius: 1.2em;
            overflow: hidden;
        }

        th {
            background-color: #14b13bcc;
            color: rgb(255, 255, 255);
        }

        .rank {
            text-align: center;
        }

        td img {
            display: block;
            margin-left: auto;
            margin-right: auto;

        }

        .special {
            text-align: left;
        }

        .spec {
            text-align: left;
        }

        th,
        td {
            font-family: 'Open Sans', sans-serif;
            text-align: center;
            color: rgb(238, 238, 238);
        }

        tr:nth-child(even) {
            background-color: #c3d8be60;
        }

        tr:nth-child(odd) {
            background-color: #adadad60;
        }

        .button {
            background: #14b13bcc;
            border-radius: 100px;
            padding: 5px 15px;
            color: #fff;
            text-decoration: none;
            font-family: 'Open Sans', sans-serif;
            border: 2px solid #0f832c;
            margin: 0 auto;
            font-weight: 700;
            width: 100px;
            text-align: left;
        }

        .button2 {
            background: #dfad0cf8;
            border-radius: 100px;
            padding: 5px 15px;
            color: #fff;
            text-decoration: none;
            font-family: 'Open Sans', sans-serif;
            border: 2px solid #aa8405f8;
            font-weight: 700;
            width: 120px;
            text-align: left;
            margin: 0rem 1rem;
            margin-top: 2rem;
            margin-bottom: 0rem;
        }

        .button2:hover {
            -webkit-animation: hover 1200ms linear 2 alternate;
            animation: hover 1200ms linear 2 alternate;
        }

        /* Active state animation applied here */
        .button2:active {
            -webkit-animation: active 1200ms ease 1 alternate;
            animation: active 1200ms ease 1 alternate;
            background: #5F9BE0;
        }

        /* Hover state animation applied here */
        .button:hover {
            -webkit-animation: hover 1200ms linear 2 alternate;
            animation: hover 1200ms linear 2 alternate;
        }

