    <link href="//maxcdn.bootstrapcdn.com/bootstrap/3.3.0/css/bootstrap.min.css" rel="stylesheet" id="bootstrap-css">
    <script src="//maxcdn.bootstrapcdn.com/bootstrap/3.3.0/js/bootstrap.min.js"></script>
    <script src="//code.jquery.com/jquery-1.11.1.min.js"></script>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">

    <div id='cssmenu'>
        <ul>
            <li><a href='/home'><span><i class="fa fa-home" style="font-size:18px"></i> Home</span></a></li>
            <li><a href='/store'><span><span class="glyphicon">&#xe116;</span> Store</span></a></li>
            <li><a href='/normal'><span> ▷ Normal Mode</span></a></li>
            <li class='active'><a href='/hacktiv'><span>▶ Hacktiv Mode</span></a></li>
            <li><a href='/performance'><span> Performance</span></a></li>
            <li class='last'><a href='/leaderboard'><span><i style="font-size:16px" class="fa">&#xf091;</i>
                        Leaderboard</span></a></li>
            <li class='right'><a href='/logout'><span>Logout</span></a></li>
        </ul>
    </div>

    <div class="row">
        <div class="main">
            <div class="game">
                <canvas id="canvas" width="850" height="475"></canvas>
            </div>
            <script>
                var myName = "<%= full_name %>";
                var cvs = document.getElementById("canvas");
                var ctx = cvs.getContext("2d");

                var bird = new Image();
                var bg = new Image();
                var fg = new Image();
                var pipeNorth = new Image();
                var pipeSouth = new Image();
                var coin = new Image()
                var turtle = new Image()
                var over = new Image()

                bird.src = "/<%= avatar %>";
                bg.src = "<%= map %>";
                fg.src = "/images/foreground.png";
                pipeNorth.src = "/images/pipeNorth2.png";
                pipeSouth.src = "/images/pipeSouth2.png";
                coin.src = "/images/coin.png"
                turtle.src = "/images/turtle.png"
                over.src = "/images/gameover2.png"

                var gap = 125;
                var constant;

                var bX = 10;
                var bY = 150;

                var gravity = 1.5;

                var score = 0;
                var money = 0

                var fly = new Audio();
                var scor = new Audio();
                var enemy = new Audio();
                var lose = new Audio();
                var soundBG = new Audio();

                fly.src = "/sounds/fly.mp3";
                scor.src = "/sounds/score.mp3";
                enemy.src = "/sounds/turtle.mp3";
                lose.src = "/sounds/lose.mp3";
                soundBG.src = "/sounds/MI.mp3";

                document.addEventListener("keydown", moveUp);

                function moveUp() {
                    bY -= 40;
                    fly.play();
                }

                var pipe = [];

                pipe[0] = {
                    x: cvs.width,
                    y: 0
                };

                let grav = false
                let check = false
                function draw() {
                    ctx.drawImage(bg, 0, 0);
                    soundBG.play()

                    for (var i = 0; i < pipe.length; i++) {

                        constant = pipeNorth.height + gap;
                        ctx.drawImage(pipeNorth, pipe[i].x, pipe[i].y);
                        ctx.drawImage(pipeSouth, pipe[i].x, pipe[i].y + constant);
                        if (i % 5 == 0) {
                            ctx.drawImage(turtle, pipe[i].x + 125, pipe[i].y + 265);
                        } else if (i % 2 == 0) {
                            ctx.drawImage(coin, pipe[i].x + 125, pipe[i].y + 265);
                        }

                        pipe[i].x -= 2

                        if (pipe[i].x == 600) {
                            pipe.push({
                                x: cvs.width,
                                y: Math.floor(Math.random() * pipeNorth.height) - pipeNorth.height
                            });
                        }

                        if (bX + bird.width - 10 >= pipe[i].x && bX <= pipe[i].x + pipeNorth.width && (bY <= pipe[i].y + pipeNorth.height || bY + bird.height >= pipe[i].y + constant) || bY + bird.height >= cvs.height - fg.height) {
                            check = true
                            soundBG.pause()
                            lose.play()
                            document.getElementById('myScore').value = score
                            document.getElementById('myMoney').value = money
                        }

                        if ((pipe[i].x + 125 >= 10 && pipe[i].x + 125 <= 30) && (bY + bird.height >= pipe[i].y + 265 && bY <= pipe[i].y + 315)) {
                            if (i % 5 == 0) {
                                ctx.fillText("-25", pipe[i].x + 125, pipe[i].y + 265);
                                money -= 25
                                enemy.play();
                                pipe[i].x = pipe[i].x - 100
                                gravity += 1
                                grav = true
                            } else if (i % 2 == 0) {
                                ctx.fillText("+25", pipe[i].x + 125, pipe[i].y + 265);
                                money += 25
                                scor.play();
                                pipe[i].x = pipe[i].x - 100
                                if (grav == true) {
                                    gravity -= 1
                                    grav = false
                                }
                            }
                        }

                        if (pipe[i].x == 4) {
                            score++
                        }
                    }

                    ctx.drawImage(fg, 0, cvs.height - fg.height);

                    ctx.drawImage(bird, bX, bY);

                    bY += gravity;

                    ctx.fillStyle = "#000";
                    ctx.font = "20px Verdana";
                    ctx.drawImage(coin, 10, cvs.height - 94);
                    ctx.fillText("x" + money, 60, cvs.height - 60);
                    ctx.fillText("Score : " + score, 10, cvs.height - 20);
                    ctx.drawImage(bird, cvs.width - 428, cvs.height - 85);
                    ctx.fillText(myName, cvs.width - 490, cvs.height - 30);

                    if (check == false) {
                        requestAnimationFrame(draw);
                    } else {
                        ctx.drawImage(over, 0, 0);
                    }
                }
                draw();
            </script>

            <div style="text-align: center;">
                <form method=POST>
                    <div id="score"></div>
                    <input type="hidden" id="myScore" name="score" value="" />
                    <input type="hidden" id="myMoney" name="money" value="" />
                    <button class="button" type="submit" name="do" value="play">
                        ▶ Play again
                    </button>
                    <button class="button" type="submit" name="do" value="home">
                        ✔ Finish
                    </button>

                </form>
            </div>

        </div>
    </div>
</body>

</html>