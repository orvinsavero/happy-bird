        .error {
            margin: 10px;
            text-shadow: 0 2px 8px 0 rgb(58, 55, 55);
            color: rgba(223, 35, 35, 0.959);

            text-align: center;
            font-family: 'Open Sans', sans-serif;
            font-weight: 700;
            margin-bottom: 15px;
        }

        body {
            font-family: Arial, Helvetica, sans-serif;
        }

        /* Full-width input fields */
        input[type=text],
        input[type=password] {
            width: 95%;
            padding: 12px 20px;
            margin: 8px 0;
            display: inline-block;
            border: 1px solid #ccc;
            box-sizing: border-box;
        }

        /* Set a style for all buttons */

        .button {
            background: #16be40cc;
            border-radius: 100px;
            padding: 5px 15px;
            color: #fff;
            text-decoration: none;
            font-family: 'Open Sans', sans-serif;
            border: 2px solid #0f832c;
            font-weight: 700;
            width: 110px;
            margin-top: 1.2rem;
        }

        .button2 {
            background: #07b2f5d7;
            border-radius: 100px;
            padding: 5px 15px;
            color: #fff;
            text-decoration: none;
            font-family: 'Open Sans', sans-serif;
            border: 2px solid #058cc2d7;
            font-weight: 700;
            width: 110px;
            margin-top: 1.2rem;
        }

        /* Hover state animation applied here */
        .button:hover {
            -webkit-animation: hover 1200ms linear 2 alternate;
            animation: hover 1200ms linear 2 alternate;
        }

        /* Active state animation applied here */
        .button:active {
            -webkit-animation: active 1200ms ease 1 alternate;
            animation: active 1200ms ease 1 alternate;
            background: #5F9BE0;
        }

        .button2:hover {
            -webkit-animation: hover 1200ms linear 2 alternate;
            animation: hover 1200ms linear 2 alternate;
        }

        /* Active state animation applied here */
        .button2:active {
            -webkit-animation: active 1200ms ease 1 alternate;
            animation: active 1200ms ease 1 alternate;
            background: #5F9BE0;
