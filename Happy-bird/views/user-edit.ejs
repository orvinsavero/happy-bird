  <link rel="stylesheet" type="text/css" href="/./css/style.css">
  <title>Profile</title>
  <style>
    input {
      width: 70%;
      border: 2px solid rgba(16, 187, 0, 0.74);
      background: none;
      position: relative;
      top: 0;
      left: 0;
      z-index: 1;
      padding: 4px 12px;
      outline: 0;
      border-radius: 40px;
    }

    input[type=text] {
      background-color: white;
      color: black;
    }

    input[type=password] {
      background-color: white;
      color: black;
    }

    body {
      background-image: url('/images/sky.png');
    }

    .head {
      font-family: 'Open Sans', sans-serif;
      font-weight: 700;
      color: rgba(255, 255, 255, 0.89);
    }

    .card {
      border-radius: 30px;
      box-shadow: 0 2px 8px 0 rgb(252, 252, 252);
      background-color: #5f5f5f9a;
      margin: auto;
      text-align: center;
      font-family: 'Open Sans', sans-serif;
      width: 300px;
    }

    .title {
      color: white;
      font-size: 14px;
    }

    table {
      border-collapse: collapse;
      width: 400px;
      margin-top: 20px
    }

    th {
      background-color: #14b13bcc;
      color: rgb(255, 255, 255);
    }

    .rank {
      text-align: center;
    }

    td img {
      display: block;
      margin-left: auto;
      margin-right: auto;

    }

    .special {
      text-align: left;
    }

    .spec {
      text-align: left;
    }

    th,
    td {
      font-family: 'Open Sans', sans-serif;
      text-align: center;
      color: rgb(238, 238, 238);
    }

    tr:nth-child(even) {
      background-color: #c3d8be60;
    }

    .button {
      background: #14b13bcc;
      border-radius: 100px;
      padding: 5px 15px;
      color: #fff;
      text-decoration: none;
      font-family: 'Open Sans', sans-serif;
      border: 2px solid #0f832c;
      margin: 0 auto;
      font-weight: 700;
      width: 100px;

    }

    .button2 {
      background: #f50707d7;
      border-radius: 100px;
      padding: 5px 15px;
      color: #fff;
      text-decoration: none;
      font-family: 'Open Sans', sans-serif;
      border: 2px solid #bd0606d7;
      margin: 0 auto;
      font-weight: 700;
      width: 100px;
      text-align: center
    }

    .button3 {
      background: #07b2f5d7;
      border-radius: 100px;
      padding: 5px 15px;
      color: #fff;
      text-decoration: none;
      font-family: 'Open Sans', sans-serif;
      border: 2px solid #058cc2d7;
      margin: 0 auto;
      font-weight: 700;

    }

    /* Hover state animation applied here */
    .button:hover {
      -webkit-animation: hover 1200ms linear 2 alternate;
      animation: hover 1200ms linear 2 alternate;
    }

    /* Active state animation applied here */
    .button:active {
      -webkit-animation: active 1200ms ease 1 alternate;
      animation: active 1200ms ease 1 alternate;
      background: #5F9BE0;
    }

    .button2:hover {
      -webkit-animation: hover 1200ms linear 2 alternate;
      animation: hover 1200ms linear 2 alternate;
    }

    /* Active state animation applied here */
    .button2:active {
      -webkit-animation: active 1200ms ease 1 alternate;
      animation: active 1200ms ease 1 alternate;
      background: #5F9BE0;
    }

    .button3:hover {
      -webkit-animation: hover 1200ms linear 2 alternate;
      animation: hover 1200ms linear 2 alternate;
    }

    /* Active state animation applied here */
    .button3.bird {
      -webkit-animation: active 1200ms ease 1 alternate;
      animation: active 1200ms ease 1 alternate;
      background: #5F9BE0;
    }

